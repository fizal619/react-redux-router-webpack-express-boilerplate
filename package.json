{
  "name": "react-router-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for creating a sample react-router application",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --colors",
    "dev": "concurrently --prefix \"[{name}]\" -k \"webpack -d --watch --colors\"  \"nodemon server.js\" -n \"WEBPACK,NODEMON\"",
    "start": "node server.js"
  },
  "author": "Fizal Sarif",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "nodemon": "^1.11.0",
    "path": "^0.12.7",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^5.0.2",
    "react-router": "^2.0.0",
    "redux": "^3.6.0",
    "style-loader": "^0.13.1"
  },
  "devDependencies": {
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "http-server": "^0.8.5",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
